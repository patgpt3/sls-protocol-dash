{
  "name": "waev-dashboard",
  "version": "0.1.0",
  "author": "Michael Burgess",
  "private": true,
  "description": "Waev Dashboard",
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/material": "5.15.0",
    "@react-jvectormap/core": "1.0.2",
    "@react-jvectormap/world": "1.0.1",
    "@reduxjs/toolkit": "1.9.0",
    "browser-fs-access": "^0.31.1",
    "chroma-js": "2.4.2",
    "dropzone": "5.9.2",
    "moment": "^2.29.3",
    "papaparse": "^5.3.2",
    "react": "18.3.1",
    "react-chartjs-2": "5.2.0",
    "react-code-blocks": "0.0.9-0",
    "react-dom": "18.3.1",
    "react-input-mask": "^2.0.4",
    "react-intl": "6.2.5",
    "react-query": "3.39.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "6.2.1",
    "react-scripts": "5.0.0",
    "react-table": "7.7.0",
    "redux": "4.2.1",
    "typescript": "4.6.2",
    "uuid": "8.3.2"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "start:local": "REACT_APP_API_URL=http://localhost:3000 GENERATE_SOURCEMAP=false react-scripts start",
    "start:staging": "REACT_APP_API_URL=https://api-staging.beaconator.com GENERATE_SOURCEMAP=false react-scripts start",
    "start:development": "REACT_APP_API_URL=https://api-development.beaconator.com GENERATE_SOURCEMAP=false react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build --legacy-peer-deps; cp .htaccess ./build/",
    "build:development": "REACT_APP_API_URL=https://api-development.beaconator.com npm run build",
    "build:staging": "REACT_APP_API_URL=https://api-staging.beaconator.com npm run build",
    "build:production": "REACT_APP_API_URL=https://api.beaconator.com npm run build",
    "formatjs:extract": "formatjs extract",
    "formatjs:compile": "formatjs compile",
    "messages:extract": "npm run formatjs:extract -- 'src/**/!(*.d).ts*' --out-file ./src/languages/messages.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "messages:compile": "npm run formatjs:compile -- ./src/languages/messages.json --ast --out-file ./src/languages/en.json",
    "translations": "npm run messages:extract && npm run messages:compile",
    "test": "REACT_APP_API_URL=http://localhost:3000 GENERATE_SOURCEMAP=false react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@formatjs/cli": "^5.1.10",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "13.5.0",
    "@types/chroma-js": "2.1.3",
    "@types/dropzone": "5.7.4",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.21",
    "@types/papaparse": "^5.3.5",
    "@types/react": "18.3.7",
    "@types/react-dom": "18.3.0",
    "@types/react-input-mask": "^3.0.2",
    "@types/react-table": "7.7.9",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "8.8.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "msw": "^0.44.2",
    "prettier": "2.5.1",
    "react-test-renderer": "^16.14.0"
  }
}
